<%-include("./components/header")-%>
<%-include("./components/nav.ejs")-%>
  <body class="  bg-gray-800 w-full h-screen relative" >
<div id="gallery" class="relative w-full sideImg z-10" data-carousel="slide">
  <!-- Carousel wrapper -->
  <div class="relative h-56 overflow-hidden rounded-lg md:h-96">
       <!-- Item 1 -->
      <div class="hidden duration-700 ease-in-out" data-carousel-item>
          <img src="https://i.pinimg.com/736x/f0/95/e5/f095e58c6f53edae9534b695b75b0170.jpg" class="absolute block max-w-full h-auto -translate-x-1/2 -translate-y-1/2 top-1/2 left-1/2" alt="">
      </div>
      <!-- Item 2 -->
      <div class="hidden duration-700 ease-in-out" data-carousel-item="active">
          <img src="https://i.pinimg.com/564x/96/72/5b/96725ba68a36b7574f8cbd3e738a30f0.jpg" class="absolute block max-w-full h-auto -translate-x-1/2 -translate-y-1/2 top-1/2 left-1/2" alt="">
      </div>
      <!-- Item 3 -->
      <div class="hidden duration-700 ease-in-out" data-carousel-item>
          <img src="https://i.pinimg.com/564x/d5/50/bb/d550bbb5c67c186ca67d1a7ce60bd90f.jpg" class="absolute block max-w-full h-auto -translate-x-1/2 -translate-y-1/2 top-1/2 left-1/2" alt="">
      </div>
      <!-- Item 4 -->
      <div class="hidden duration-700 ease-in-out" data-carousel-item>
          <img src="https://i.pinimg.com/736x/d5/7c/12/d57c12e6446c15db9f8e62d2753714bb.jpg" class="absolute block max-w-full h-auto -translate-x-1/2 -translate-y-1/2 top-1/2 left-1/2" alt="">
      </div>
      <!-- Item 5 -->
      <div class="hidden duration-700 ease-in-out" data-carousel-item>
          <img src="https://i.pinimg.com/736x/03/d5/22/03d5227d28847dd437e6e078e06b917b.jpg" class="absolute block max-w-full h-auto -translate-x-1/2 -translate-y-1/2 top-1/2 left-1/2" alt="">
      </div>
  </div>
  <!-- Slider controls -->
</div>
    <!-- thumbkins  -->
<div class="container itemsHi  w-full h-auto flex   flex-wrap justify-center gap-2 ">
<% data.forEach(element => { %>
  <div class=" mt-3  h-60  md:h-80 md:w-80  rounded-xl   bg-slate-500 bg-slate-300">
    <a href="/show/<%= element._id%>">
      <img src="<%= element.thumbnailLink %> " class="h-full w-full rounded-xl" alt="">
    </a>
</div>
<% }) %>
</div>
<%- include('./components/footer.ejs') -%>
<script src="https://cdnjs.cloudflare.com/ajax/libs/flowbite/2.3.0/flowbite.min.js"></script>
<script src="/script/index.js"></script>
  </body>
</html>
